//Inicializa los motores
void initMxSub(){
	M1_ESC_Enable();
	M2_ESC_Enable();
	M3_ESC_Enable();
	M4_ESC_Enable();
}

//Cambia el Status de un LED
//Hace que se mantenga encendido o titilando si està activo.
void lStatus(byte nLed, bool status)
{
	switch(nLed)
	{
		case 0x00:
			
			break;
			
		case 0x01:
					
			break;
			
		case 0x02:
							
			break;
		
		case 0x03:
							
			break;
			
		case 0x04:
							
			break;
		
		case 0x05:
					
			break;
					
		case 0x06:
							
			break;
			
		case 0x07:
									
			break;
				
	}
}

