#ifndef ONEWIRE_H_
#define ONEWIRE_H_

// Configure UART for 1-Wire RESET timing
byte onewireReset();
byte onewireWriteByte(byte);
byte onewireReadByte();
byte onewireBit(bit);
// Wake up every 5 seconds and write to the server
word awakeAndGetTemp();


#endif /* ONEWIRE_H_ */
